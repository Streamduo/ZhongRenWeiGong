<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/distance_dp_35">

    <TextView
        android:id="@+id/te_integra_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/distance_dp_4"
        android:textColor="@color/FF000000"
        android:textSize="@dimen/distance_dp_15"
        tools:text="上传好人好事视频" />

    <TextView
        android:id="@+id/te_integra_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/distance_dp_10"
        android:layout_marginRight="@dimen/distance_dp_56"
        android:textColor="@color/app_text_99"
        android:textSize="@dimen/distance_dp_14"
        tools:text="2019.10.11" />

    <TextView
        android:id="@+id/te_integra_branch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/distance_dp_10"
        android:textColor="@color/FF000000"
        android:textSize="@dimen/distance_dp_16"
        tools:text="+3" />

</RelativeLayout>